  "default_attributes": {
    "common": {
      "auth": {
        "users": {
          "data_bag": "auth_users"
        },
        "groups": {
          "data_bag": "auth_groups",
          "config": {
            "devops": {
              "action": "create"
            }
          }
        }
      },
      "sudoers": {
        "ubuntu": {
          "user": "ubuntu",
          "runas": "root",
          "nopasswd": true
        },
        "deploy": {
          "group": "deploy",
          "runas": "root",
          "nopasswd": true,
          "commands": [
            "/usr/bin/chef-client"
          ]
        },
        "devops": {
          "group": "devops",
          "runas": "root"
        },
        "vagrant": {
          "group": "vagrant",
          "runas": "root",
          "nopasswd": true
        },
        "90-cloud-init-users": {
          "action": "remove"
        }
      }
    },
    "openssh": {
      "server": {
        "allow_groups": [
          "devops",
          "deploy",
          "vagrant"
        ]
      }
    }
  },
  "override_attributes": {
    "common": {
      "environments": {
        "active": [
          "test"
        ]
      },
      "namespaces": {
        "active": [
          "test"
        ]
      }
    }
  },
